{
  "title": "JSON schema for Blue Yonder Team Secret Group",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "$ref": "#/definitions/repoSecretAssociation"
  },
  "definitions": {
    "repoSecretAssociation": {
      "type": "object",
      "required": [
        "name", "repos"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "repos": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
